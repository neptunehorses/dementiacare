{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/styles\\.css",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            },
            "dest": "/public/styles.css"
        },
        {
            "src": "/examination\\.css",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            },
            "dest": "/public/examination.css"
        },
        {
            "src": "/examination\\.js",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            },
            "dest": "/public/examination.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
} 